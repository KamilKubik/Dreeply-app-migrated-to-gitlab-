{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useRef, useEffect } from 'react';\nimport { isMobile } from 'react-device-detect';\nimport gsap, { Linear } from 'gsap';\nimport Router from 'next/router';\n\nvar ExitComponent = function ExitComponent() {\n  var exitImage = useRef();\n  useEffect(function () {\n    if (!isMobile) {\n      exitImage.current.addEventListener('mouseenter', function () {\n        gsap.to(exitImage.current, {\n          rotation: '180_cw',\n          duration: 0.5,\n          ease: Linear.easeIn\n        });\n      });\n      exitImage.current.addEventListener('mouseleave', function () {\n        gsap.to(exitImage.current, {\n          rotation: '0_cw',\n          duration: 0.5,\n          ease: Linear.easeIn\n        });\n      });\n    }\n  }, []);\n  return /*#__PURE__*/_jsx(\"img\", {\n    ref: exitImage,\n    onClick: function onClick() {\n      return Router.push(\"/dashboard/projects/\".concat(Router.query.project));\n    },\n    src: \"/business-model/back4.svg\",\n    height: 28,\n    width: 28,\n    className: \"fixed left-6 top-3 cursor-pointer z-50\"\n  });\n};\n\nexport default ExitComponent;","map":null,"metadata":{},"sourceType":"module"}